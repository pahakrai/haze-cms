      "type": "textEditor",
      "data": {
        "text": "<div>退貨、退款或取消訂單</div>\n"
      },
      "properties": [
        {
          "_id": "5e057ab7a2d2f3e559c602ff",
          "type": "boxed",
          "data": {
            "type": "inherit",
